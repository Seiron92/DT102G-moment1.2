<!DOCTYPE html>
    <html lang="sv">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>@ViewData["Title"]</title>
        <link rel="stylesheet" href="~/css/Styles.css" />
        <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700&display=swap" rel="stylesheet">
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"
                integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
                crossorigin="anonymous"></script>
    </head>
    <body>
        <div class="container">
            <div class="header">
                <h1 id="logo"><a href="/">Rebecca Seiron</a></h1>
                <div id="date"><p>Dagens datum är: @DateTime.Now.ToString("yyyy/MM/dd")</p></div>
            </div>

            <nav>
                <ul>
                    <li id="start"><a href="/">Startsida</a></li>
                    <li id="about"><a href="/About">Om mig</a></li>
                    <li id="razor"><a href="/Razor">Razor</a></li>
                </ul>
            </nav>
            <main>
                @RenderBody()
            </main>

        </div>
        <footer>
            <p>Av Rebecca Seiron - Kontakta mig via e-post: rebecca.seiron@hotmail.com</p>
        </footer>
        <script src="~/js/main.js"></script>
    </body>
</html>

